
double kp = 40.0;
double kd = 0.9; 
double ki = 0.9;

int OUTMAX = 250;
int OUTMIN = -250;

double SetPoint = -6.0;

float lastInput = 0.0;

double ITerm = 0.0;

double Compute(double input){

  /*Calculo do Erro*/
  double erro = SetPoint - input;

  ITerm+= (ki * erro);

  if(ITerm > OUTMAX){
    ITerm = OUTMAX;
  }else if(ITerm <= OUTMIN){
    ITerm = OUTMIN;
  }


  /* DiferenÃ§a da entrada atual - anterior*/
  double dInput = input - lastInput;

  /* Calculo do PID */
  double output = kp * erro + ITerm + kd * dInput;


  if(output > OUTMAX){
    output = OUTMAX;
  }else if(output <= OUTMIN){
    output = OUTMIN;
  }

  lastInput = input;
